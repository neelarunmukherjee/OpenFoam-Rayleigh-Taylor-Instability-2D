FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
defaultFieldValues
(
    volScalarFieldValue c 1
);

regions
(
	
	boxToCell
	{
		box (0  0 -0.1) (1 0.5 0.1);					
		fieldValues
		(
			volScalarFieldValue c 0
		);
	}
);


